{
  "name": "nestjs-react-mongodb-template",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/main.js",
  "author": "async-devil",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/async-devil/nestjs-react-mongodb-template.git"
  },
  "bugs": {
    "url": "https://github.com/async-devil/nestjs-react-mongodb-template/issues"
  },
  "keywords": [
    "MongoDB",
    "Mongoose",
    "Node.js",
    "Nestjs",
    "React",
    "Fullstack"
  ],
  "scripts": {
    "prebuild": "rm -rf dist && rm -rf build",
    "build:server": "tsc --project ./server/config/tsconfig.server.json",
    "build:client": "node client/scripts/build.js",
    "build": "yarn build:client && yarn build:server",
    "start:prod-server": "cross-env NODE_ENV=production node dist/main",
    "start": "yarn build && yarn start:prod-server",
    "start:dev-server": "cross-env NODE_ENV=development nest start --watch --path \"./server/config/tsconfig.server.json\"",
    "start:dev-client": "node client/scripts/start.js",
    "start:dev": "concurrently --kill-others-on-fail \"yarn start:dev-server\" \"yarn start:dev-client\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.0",
    "axios": "^0.25.0",
